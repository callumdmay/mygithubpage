{
  "name": "Website",
  "version": "1.0.0",
  "author": "Callum May",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0",
    "webpack": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "browser-sync": "^2.18.13",
    "concurrently": "^3.3.0",
    "eslint": "3.16.1",
    "eslint-plugin-react": "6.10.0",
    "gh-pages": "0.12.0",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-watch": "1.0.0",
    "grunt-sass": "2.0.0",
    "webpack-dev-server": "^2.8.2"
  },
  "homepage": "www.callummay.me",
  "repository": {
    "type": "git",
    "url": "https://github.com/callumdmay/mygithubpage.git"
  },
  "scripts": {
    "build": "grunt build && webpack --progress --colors",
    "browsersync": "browser-sync start --server --serveStatic 'dist' --no-notify --no-open --files 'dist/*'",
    "webpack:watch": "webpack --progress --colors --watch",
    "start": "grunt build && concurrently -k -r \"grunt watch\" \"webpack --progress --colors --watch\" \"npm run browsersync\"",
    "deploy": "grunt build && gh-pages -d dist -t true"
  },
  "title": "Personal Website"
}
