{
  "name": "Website",
  "version": "1.0.0",
  "author": "Callum May",
  "dependencies": {
    "grunt-contrib-copy": "^1.0.0",
    "react": "^15.6.1",
    "react-router": "^4.2.0",
    "webpack": "^3.6.0"
  },
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "concurrently": "^3.3.0",
    "gh-pages": "0.12.0",
    "grunt": "1.0.1",
    "grunt-contrib-watch": "1.0.0",
    "grunt-sass": "2.0.0"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "build"
    ],
    "noprompt": false
  },
  "homepage": "www.callummay.me",
  "repository": {
    "type": "git",
    "url": "https://github.com/callumdmay/mygithubpage.git"
  },
  "scripts": {
    "build": "gulp clean && gulp",
    "deploy": "gulp clean && gulp && gh-pages -d dist -t true",
    "start": "concurrently -k -r \"grunt sass && grunt watch\" \"browser-sync start --server --no-notify --no-open --files '*.css, *.html'\""
  },
  "title": "Personal Website"
}
